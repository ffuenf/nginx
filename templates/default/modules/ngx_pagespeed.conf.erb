# Generated by Chef for node <%= node["fqdn"] %>
# Local modifications will be overwritten.
#
# The ngx_pagespeed configuration file.
<% if @params['canonicalize_javascript_libraries'] %>
include pagespeed_libraries.conf;
<% end %>
pagespeed <%= @params['enabled'] %>;
pagespeed FileCachePath <%= @params['FileCachePath'] %>;
pagespeed CacheFlushFilename <%= @params['CacheFlushFilename'] %>;
pagespeed RewriteLevel <%= @params['RewriteLevel'] %>;
pagespeed EnableFilters <%= @params['EnableFilters'] %>;
<% if @params['DisableFilters'] %>
pagespeed DisableFilters <%= @params['DisableFilters'] %>;
<% end %>

pagespeed Statistics <%= @params['Statistics'] %>;
pagespeed StatisticsLogging <%= @params['StatisticsLogging'] %>;
pagespeed LogDir <%= @params['LogDir'] %>;
pagespeed StatisticsLoggingIntervalMs <%= @params['StatisticsLoggingIntervalMs'] %>;
pagespeed StatisticsLoggingMaxFileSizeKb <%= @params['StatisticsLoggingMaxFileSizeKb'] %>;
pagespeed MessageBufferSize <%= @params['MessageBufferSize'] %>;

pagespeed AdminPath <%= @params['AdminPath'] %>;
pagespeed GlobalAdminPath <%= @params['GlobalAdminPath'] %>;
pagespeed StatisticsPath <%= @params['StatisticsPath'] %>;
pagespeed GlobalStatisticsPath <%= @params['GlobalStatisticsPath'] %>;
pagespeed MessagesPath <%= @params['MessagesPath'] %>;
pagespeed ConsolePath <%= @params['ConsolePath'] %>;

pagespeed FileCacheSizeKb <%= @params['FileCacheSizeKb'] %>;
pagespeed FileCacheCleanIntervalMs <%= @params['FileCacheCleanIntervalMs'] %>;
pagespeed FileCacheInodeLimit <%= @params['FileCacheInodeLimit'] %>;
pagespeed LRUCacheKbPerProcess <%= @params['LRUCacheKbPerProcess'] %>;
pagespeed LRUCacheByteLimit <%= @params['LRUCacheByteLimit'] %>;
pagespeed CacheFlushPollIntervalSec <%= @params['CacheFlushPollIntervalSec'] %>;
pagespeed DownstreamCacheRewrittenPercentageThreshold <%= @params['DownstreamCacheRewrittenPercentageThreshold'] %>;
pagespeed RetainComment "<%= @params['RetainComment'] %>";
pagespeed DownstreamCachePurgeMethod <%= @params['DownstreamCachePurgeMethod'] %>;
pagespeed DownstreamCacheRebeaconingKey <%= @params['DownstreamCacheRebeaconingKey'] %>;

<% if @params['Disallow'] %>
pagespeed Disallow <%= @params['Disallow'] %>;
<% end %>
